{
  "name": "InfluxDB v2",
  "version": "v2.0.7",
  "slug": "influxdb2",
  "description": "Scalable datastore for metrics, events, and real-time analytics",
  "arch": ["amd64", "armv7"],
  "image": "taapie/hassio-influxdb2",
  "startup": "services",
  "boot": "auto",
  "url": "https://docs.influxdata.com/influxdb/v2.0",`
  "map": [
    "share:rw"
  ],
  "ports": {
    "8086/tcp": 8086
  },
  "ports_description": {
    "8086/tcp": "InfluxDB server"
  },
  "options": {
    "auth": true,
    "reporting": true,
    "envvars": []
  },
  "schema": {
    "auth": "bool",
    "reporting": "bool",
    "envvars": [
      {
        "name": "match(^INFLUXDB_([A-Z0-9_])+$)",
        "value": "str"
      }
    ]
  }
}
