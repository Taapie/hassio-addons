{
  "name": "NZBGet",
  "version": "0.1",
  "slug": "nzbget",
  "description": "Efficient Usenet downloader",
  "arch": ["armhf", "armv7", "aarch64", "amd64"],
  "url": "https://nzbget.net/",
  "startup": "application",
  "ports": {
    "6789/tcp": 6789
  },
  "ports_description": {
    "6789/tcp": "Web interface"
  },
  "apparmor": true,
  "map": [
    "share:rw",
    "ssl:ro"
  ],
  "environment": {
    "PUID": "1000",
    "PGID": "1000"
  },
  "options": {
    "TZ": "Europe/Amsterdam",
    "Server": "",
    "Port": 563,
    "Username": "",
    "Password": "",
    "SSL": true,
    "Connections": 20,
    "Retention": 0
  },
  "schema": {
    "TZ": "str",
    "Server": "str",
    "Port": "port",
    "Username": "str",
    "Password": "password",
    "SSL": "bool",
    "Connections": "int(1,)",
    "Retention": "int(0,)"
  },
  "ingress": true,
  "ingress_port": 6789,
  "panel_icon": "mdi:cloud-download",
  "panel_title": "NZBGet",
  "panel_admin": true,
  "backup": "hot",
  "backup_pre": "",
  "backup_post": "",
  "backup_exclude": ""
}
