{
    "name": "Caddy",
    "version": "0.10.10-4",
    "slug": "caddy",
    "description": "Caddy",
    "url": "https://github.com/Taapie/hassio-addons/tree/master/caddy",
    "startup": "system",
    "boot": "auto",
    "image": "taapie/{arch}-caddy",
    "options": {
	"homeassistant": "homeassistant.domain.tld",
	"vhosts": [],
	"raw_config": [],
	"email": null
    },
    "schema": {
	"vhosts": [
	    {"vhost": "str",
	     "remote": "str?",
	     "port": "str",
             "insecure": "bool?",
	     "directory": "str?",
	     "user": "str?",
	     "pwd": "str?"
	    }
	],
	"email": "email",
	"disable_http_challenge": "bool?",
	"raw_config": [{"line": "str"}],
	"homeassistant": "str?"
    },
    "ports": {"80/tcp": 80, "443/tcp": 443},
    "map": ["ssl:rw", "share:rw"]
}
